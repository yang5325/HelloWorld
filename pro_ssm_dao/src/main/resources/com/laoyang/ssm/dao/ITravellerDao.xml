<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.laoyang.ssm.dao.ITravellerDao">

    <select id="findById" resultType="com.laoyang.ssm.domain.Traveller">
        select *from traveller where id in (select travellerId from order_traveller where orderId = #{id})
    </select>

</mapper>